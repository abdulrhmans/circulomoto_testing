<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="cmoto_marca_field_view" model="ir.ui.view">
			<field name="name">product.template_campomarca.inherited</field>
			<field name="model">product.template</field>
			<field name="inherit_id" ref="product.product_template_only_form_view"></field>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='barcode']" position="after">
					<field name="x_cmoto_marca"></field>
				</xpath>
			</field>
		</record>
	</data>
</odoo>